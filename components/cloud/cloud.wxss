  width: 100%;
  height: 100%;
  border-radius: 50%;
}

.honghong_yun {
  width: 100%;
  height: 220rpx;
}

.honghong_yun .honghong_yun_img {
  width: 100%;
  height: 100%;
}

.honghong_boxs {
  width: 72%;
  position: absolute;
  top: 30rpx;
  left: 0;
  right: 0;
  margin: auto;
}

.honghong_photo {
  width: 68rpx;
  height: 68rpx;
  margin: 0 auto;
}

.honghong_photo .honghong_photo_img {
  width: 100%;
  height: 100%;
}

