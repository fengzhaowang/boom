}

.honghong_xiang {
  width: 5%;
  margin: auto;
  margin-top: -15rpx;
}

.honghong_line {
  width: 10px;
  height: 22px;
}

.honghong_line .honghong_line_img {
  width: 100%;
  height: 100%;
}

.honghong_text {
  margin-top: -5px;
  padding: 5px 5px 0px 10px;
  background: #fff;
  text-overflow: ellipsis;
  text-overflow: -o-ellipsis-lastline;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  font-size: 24rpx;
  font-family: MicrosoftYaHei;
  color: rgba(87, 87, 87, 1);
  line-height: 35rpx;
  overflow: hidden;
  word-break: break-all;
}

