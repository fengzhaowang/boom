.honghong_content_yun {
  width: 50%;
  height: auto;
  position: absolute;
  font-family: MicrosoftYaHei;
  animation: 10s cloud_move ease-in-out;
}

.honghong_content {
  height: auto;
  overflow: hidden;
  
}

.honghong_photo {
  width: 34px;
  height: 34px;
}

